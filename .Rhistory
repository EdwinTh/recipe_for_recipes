signed_log <- function(x) {
ifelse(abs(x) < 1, 0, sign(x) * log(abs(x)))
}
signed_log(-10)
signed_log(-10:10)
signed_log(-1000:1000)
signed_log <- function(x, base = exp(1)) {
ifelse(abs(x) < 1, 0, sign(x) * log(abs(x), base = base))
}
signed_log(199)
signed_log(-199)
